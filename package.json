		"url": "https://github.com/ljharb/is-arrow-function/issues"
	},
	"dependencies": {
		"is-callable": "^1.2.0"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^17.1.0",
		"aud": "^1.1.2",
		"eslint": "^7.2.0",
		"for-each": "^0.3.3",
		"make-arrow-function": "^1.2.0",
		"make-async-function": "^1.0.0",
		"make-generator-function": "^2.0.0",
